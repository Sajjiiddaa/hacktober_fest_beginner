#include <iostream>

using namespace std;

int main()
{
    int number;

    cout << "Enter a positive integer: ";
    cin >> number

    cout << "Factors of " << n << " are: " << endl;  
    for(int i = 1; i <= number; i++)
    {
        if(number % i == 0) {
            cout << i << endl;
        }
    }

    return 0;
}
